@model MyApp.Models.Category
@{
    ViewData["Title"] = "Delete";
}

<div class="container mt-5">
    <div class="row p-2">
        <div class="col-8">
           Delete Category
        </div>
        <div class="col-4">
            <a asp-action="Index" asp-controller="Category" class="btn btn-primary"><i class="bi bi-backspace-reverse-fill"></i>&nbsp Back to List</a>
        </div>

    </div>
    <form method="post">
        <input asp-for="id" type="hidden" />
        <div class="form-group">
            <label asp-for="Name"></label>
            <input disabled asp-for="Name" class="form-control" required>
        </div>

        <div class="form-group">
            <label asp-for="DisplayOrder">Display Order</label>
            <input disabled asp-for="DisplayOrder    " class="form-control">
        </div>

       

        <button type="submit" class="btn btn-primary">Delete</button>
    </form>


</div>


